programa {
  funcao inicio() { 
    inteiro numA, numB
    inteiro resultado

    escreva ("digite o valor do numero A: ")
    leia (numA)

    escreva ("digite o valor do numero B: ")
    leia (numB) 

    resultado = (numA - numB) * (numA - numB)

    escreva("O resultado do quadrado diferente é: ", resultado)
  }
}
